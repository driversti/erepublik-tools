{
  "name": "erepublik-statistics",
  "version": "1.0.0",
  "description": "Saving profile statistics to an Excel file",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "webpack",
    "chmod": "chmod +x ./create_buildx.sh",
    "createBuilder": "npm run chmod && ./create_buildx.sh",
    "generateDockerfile": "node generateDockerfile.js",
    "buildVersion": "docker buildx build --platform linux/amd64,linux/arm64 -t driversti/erep-profile-stats:${npm_package_version} --push .",
    "buildLatest": "docker buildx build --platform linux/amd64,linux/arm64 -t driversti/erep-profile-stats:latest --push .",
    "build": "npm run generateDockerfile && npm run createBuilder && npm run buildVersion && npm run buildLatest",
    "package": "npm run bundle && npm run build"
  },
  "author": "driversti",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "jest": "^29.5.0",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@json2csv/plainjs": "^7.0.1",
    "axios": "^1.4.0",
    "axios-cookiejar-support": "^4.0.6",
    "cheerio": "^1.0.0-rc.12",
    "deasync": "^0.1.28",
    "exceljs": "^4.3.0",
    "fast-csv": "^4.3.6",
    "fs": "^0.0.1-security",
    "papaparse": "^5.4.1",
    "qs": "^6.11.2",
    "tough-cookie": "^4.1.3",
    "url": "^0.11.0"
  }
}
